{
  "scripts": {
    "start": "next dev",
    "now-build": "next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js|jsx|json}": [
        "standard --fix",
        "prettier-standard",
        "git add"
      ]
    }
  },
  "dependencies": {
    "next": "^8.0.0-canary.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier-standard": "^8.0.1",
    "standard": "^12.0.1"
  }
}
